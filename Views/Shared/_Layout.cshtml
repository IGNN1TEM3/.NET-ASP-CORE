<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/font-awesome/css/ all.min.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <link rel="stylesheet" href="~/WEB_253502_BARANOVSKY.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
    <div class="container">
        <nav class="navbar-dark bg-dark">
            <div class="d-flex justify-content-between align-items-center">
                <partial name="_Navbar" />
                <partial name="_UserInfo" />
            </div>
        </nav>  
    </div>

    </header>

        @RenderSection("Main", required:true)
    <footer>
    <div class="navbar navbar-dark navbar-expand-md bg-dark">
        <div class="navbar-nav ms-auto">
            <a class="nav-item nav-link" href="http://www.facebook.com">
                <i class="fa-brands fa-facebook-f fa-2x"></i>
            </a>
            <a class="nav-item nav-link" href="http://www.vk.com">
                <i class="fa-brands fa-vk fa-2x"></i>
            </a>
            <a class="nav-item nav-link" href="http://www.telegram.com">
                <i class="fa-brands fa-telegram fa-2x"></i>
            </a>
        </div>
    </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)   
</body>
</html>
